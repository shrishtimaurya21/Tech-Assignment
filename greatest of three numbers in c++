#include <iostream>
using namespace std;
int main()
{
    int n1, n2, n3;
    cout << "Enter the first number:";
    cin >> n1;
    cout << "Enter the second number:";
    cin >> n2;
    cout << "Enter the third number: ";
    cin >> n3;
    if (n1 >= n2 && n1 >= n3)
    {
        cout << n1 << "is greater number" << endl;
    }
    else if (n2 >= n1 && n2 >= n3)
    {
        cout << n2 << "is greater number" << endl;
    }
    else
    {
        cout << n3 << "is greater number" << endl;
    }
    return 0;
}
